{
	"buildOptions": {
		"emitEntryPoint": false,
		"xmlDoc": true,
		"platform": "anycpu",
		"additionalArguments": [
			"/ruleset:../StrongGrid.ruleset",
			"/additionalfile:../stylecop.json"
		],
		"compile": {
		}
	},
	"configurations": {
		"Release": {
			"buildOptions": {
				"warningsAsErrors": true
			}
		}
	},
	"dependencies": {
		"StyleCop.Analyzers": {
			"version": "1.0.0",
			"type": "build"
		}
	},
	"frameworks": {
		"netstandard1.3": {
			"buildOptions": {
				"define": [ "NETSTANDARD" ]
			},
			"imports": "dnxcore50",
			"dependencies": {
				"NETStandard.Library": "1.6.1",
				"Newtonsoft.Json": "9.0.1",
				"System.Reflection.TypeExtensions": "4.1.0"
			}
		},
		"net452": {
			"buildOptions": {
				"define": [ "NET452" ]
			},
			"dependencies": {
				"Newtonsoft.Json": "9.0.1",
				"System.Net.Http": "4.0.0"
			},
			"frameworkAssemblies": {
				"System.Runtime.Serialization": "4.0.0"
			}
		}
	},
	"version": "0.13.0-*"
}
